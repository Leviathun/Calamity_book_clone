    <%- include('../templates/nav-bar-user') %>

    <!--Best seller container start-->
    <!-- Best seller start-->
    <div class="containers wraper" style="margin-top: 50px; margin-bottom: 50px;">
    
        <div class="bannerTopic">
          <div class="d-flex flex-column flex-lg-row center">
              <h2 class="filterflashsale-title">
                Contact us
              </h2>                
            </div>
            
            <!--<div class="btnViewall">
              <a href="#">View all <i class="fas fa-chevron-right fa-xs"></i></a>
            </div>-->
  
        </div>
        <!-- Best seller end-->
        
        <div class="row top-contact">

            <div class="col-sm-12 col-lg-6 center">        
                <div class="Api">             
                    <iframe width="600" height="400" style="border:0" loading="lazy" allowfullscreen src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJs95C9Ws62jARRASSMzCs-4U&key=AIzaSyAo0v1Gwk0scXPfXZNWOviHih1j-652V9w"></iframe>          
                </div>
            </div>

            <div class="col-sm-12 col-lg-6 ">
                <div class="info">
                    <h4>Calamity Bookshop</h4>
                    <h4>Address : 239 Huay Kaew Road, Suthep Subdistrict, Mueang Chiang Mai District, Chiang Mai 50200</h4>
                    <h4>Follow us : 
                        <a class="link-body-emphasis ms-4" href="https://www.facebook.com"><i class="fa-brands fa-facebook"></i></a>
                        <a class="link-body-emphasis ms-4" href="https://www.instagram.com"><i class="fa-brands fa-instagram"></i></a>
                        <a class="link-body-emphasis ms-4" href="https://twitter.com"><i class="fa-brands fa-x-twitter"></i></a>
                    </h4>        
                    <h4>Email : Calamity@gmail.com</h4>
                </div>
            </div>             
        </div>

        <div class="row top-contact mt-5">
            <div class="center mt-4">
                <h2>Let talk with us</h2>
            </div>
            <div class="talk">
                <form action="" method="post">
                    <input type="email" name="email" placeholder="your email" class="contact-inputs" required>
                    <br>
                    <textarea name="massage" placeholder="let talk..." class="contact-inputs" required></textarea>
                    <br>
                    <button class="submit" type="submit" >Submit</button>
                </form>
            </div>

        </div>
  
        
      </div>
      <!--Best seller container end-->
           
      <%- include('../templates/user-footer') %>

