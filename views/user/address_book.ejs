<body>
    <!-- Nav-Bar Main -->
    <%- include('../templates/nav-bar-user') %>
    <div class="container2">
    <div class="row">
      
            <!-- Side-Bar Top -->
            <div class="col-lg-6 col-md-12 col-12">
            <div class="sidebartop">
               
                <!-- Side-Bar Heading -->
                <div class="heading">
                    <h1>Welcome</h1>
                </div>
                <!-- Side-Bar Heading -->
    
                <!-- Open Side-Bar -->
                <div class="sidebar-button">
                    <button class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#sidebar1">
                        <i class="fas fa-bars"></i>
                    </button>
                </div>
                <!-- Open Side-Bar -->
                
            </div>
            <!-- Side-Bar Top -->
    
            <!-- Side-Bar Menu -->
            <div id="sidebar1" class="collapse navbar-collapse show">
                <!-- Side-Bar List -->
                <ul id="navbar2">
                    <li id="info"><a href="/account-info" class="nav-link navbar-item item1"><i class="fas fa-user"></i> Account Information</a></li>
                    <li id="info"><a href="/address-book" class="nav-link navbar-item item2"><i class="fas fa-address-book"></i> Address Book</a></li>
                    <li id="info"><a href="/my-order" class="nav-link navbar-item item3"><i class="fas fa-shopping-cart"></i> My Order</a></li>
                    <li id="info"><a href="/wishlist" class="nav-link navbar-item item4"><i class="fas fa-heart"></i> My Wishlist</a></li>
                    <li id="info"><a href="/track-order" class="nav-link navbar-item item5"><i class="fas fa-truck"></i> Track My Order</a></li>
                    <li id="info"><a href="/sign-out" class="nav-link navbar-item item6"><i class="fas fa-sign-out-alt"></i> Sign Out</a></li>
                </ul>
                <!-- Side-Bar List -->
               
            </div>
            <!-- Side-Bar Menu -->
        </div>
     
    <!-- Side-Bar -->


<!-- Main Side-Bar -->

<!-- Content -->

<div class="col-lg-6 col-md-12 col-12">
    <div class="accountin">

        <div class="content-header">
            <h1>Address Book</h1>
        </div>

        <div class="form">
            <div>
                <input class="input2" type="text" id="inputState" placeholder="State or Province"> 
            </div>     

            <div>
                <input class="input2" type="text" id="inputPostalCode" placeholder="Zip/Postal code"> 
            </div>   

            <div>
                <input class="input2" type="text" id="inputCity" placeholder="City or town"> 
            </div>      

            <div>
                <input class="input2" type="text" id="inputBuildingNo" placeholder="House/Building No"> 
            </div>

            <div>
                <input class="input2" type="text" id="inputStreet" placeholder="Street"> 
            </div>

            <div>
                <input class="input2" type="text" id="inputCountry" placeholder="Country"> 
            </div>

            <div class="button" id="submitButton">
                <button>Save address</button>
            </div>

        </div>

    </div>
</div>
</div>
</div>
<!-- Content -->
<script src="js/home.js"></script>
<script
